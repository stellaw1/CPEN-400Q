    "import pennylane as qml\n",
    "from pennylane import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "coeffs = [0.5, -0.5]\n",
    "ops = [qml.PauliZ(0) @ qml.PauliY(1), qml.Identity(0) @ qml.PauliY(1)]\n",
    "\n",
    "H = qml.Hamiltonian(coeffs, ops)"
